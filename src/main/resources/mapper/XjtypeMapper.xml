<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.XjtypeMapper">
    <resultMap id="xjtypeEntityMa" type="com.example.demo.bean.Xjtype">
        <result property="typeid" column="typeid"></result>
        <result property="typename" column="typename"></result>
    </resultMap>
    <select id="getalltype" resultMap="xjtypeEntityMa">
        SELECT * FROM xjtype
    </select>

    <insert id="insert" keyProperty="typeid">
        insert into xjtype(typename) values (#{typename})
    </insert>

    <delete id="deletebytypeid">
        delete from xjtype where typeid=#{typeid}
    </delete>
</mapper>
